generator client {
  provider = "prisma-client-js"
  output   = "../node_modules/.prisma/client"
}

datasource db {
  provider  = "postgresql"
}

enum Role {
  admin
  jobseeker
}

model User {
  id        String   @id @default(uuid())
  fullName  String
  email     String   @unique
  password  String
  role      Role     @default(jobseeker)
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
